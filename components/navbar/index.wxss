.nav-wrap{position:fixed;width:100%;top:0;background:#ffffff;color:#000;z-index:999;}

.nav-bar{display:flex;justify-content:space-around;flex-direction:column}
.nav-title{position:absolute;text-align:center;max-width:400rpx;top:0;left:0;right:0;bottom:0;margin:auto;font-size:32rpx;color:#2b2b2b;font-weight:400;}
.nav-capsule{display:flex;align-items:center;margin-left:30rpx;width:140rpx;justify-content:space-between;height:100%;}
.backBtn{width: 80rpx;height: 36rpx;padding-top: 12rpx;}
.back-pre{width:36rpx;height:36rpx;}


.navbar-v-line{width:1px;height:32rpx;background-color:#e5e5e5;}
.back-home{width:36rpx;height:36rpx;padding:10rpx;}
.nav-capsule .back-home{width:36rpx;height:40rpx;margin-top:3rpx;}