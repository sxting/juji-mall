.main_cardlist {
  padding: 30rpx 30rpx 100rpx 30rpx;
  box-sizing: border-box;
  min-height: 100%;
  position: relative;
  z-index: 99999;
}

.cardsWrap {
  width: 100%;
}

.cardItem {
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
  height: 430rpx;
  border-radius: 8rpx;
  margin-bottom: 30rpx;
  transition: height 0.3s;
  overflow: hidden;
}

.cardInfo {
    height: 120rpx;
    padding-top: 30rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.shopLogo {
    width: 79rpx;
    height: 79rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.cardInfoRow {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.shopName {
    font-size: 30rpx;
    color: #6A3C12;
}

.ncardNo {
    font-size: 22rpx;
    color: #6A3C12;
}

.cardType {
    font-size: 22rpx;
    color: #6A3C12;
}

.blanceRow {
    width: 100%;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 30rpx;
    box-sizing: border-box;
}

.blanceTitle {
    font-size: 24rpx;
    color: #6A3C12;
}
.blanceWrap {
    display: flex;
    align-items: center;
}
.blanceMark {
    font-size: 60rpx;
    color: #6A3C12;
    margin-right: 10rpx;
}

.blanceText {
    font-size: 80rpx;
    color: #6A3C12;
}

.btnGroup view {
  color: #6a3c12;
  font-size: 28rpx;
}


.cardbtn {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 30rpx;
  text-align: left;
  box-sizing: border-box;
}

.shuiyin {
  position: absolute;
  bottom: 50rpx;
  left: 50%;
  margin-left: -60rpx;
}
