.page{width:100%;height:100%;overflow:hidden;position:relative}
.page image{width:100%;height:100%}
.btn-manger{position:fixed;font-family:PingFangSC-Medium;font-size:24rpx;color:#6A3C12;text-align:center;line-height:50rpx;background-color:#FFC520;top:18rpx;right:0;width:140rpx;height:50rpx;border-radius:25rpx 0 0 25rpx;}
.applyToManager{position:absolute;font-family:PingFangSC-Semibold;font-size:30rpx;color:#FFFFFF;text-align:center;top:489rpx;line-height:80rpx;width:460rpx;height:80rpx;left:50%;margin-left:-230rpx;}
.applyToManager .iconfont{font-size:38rpx;font-weight:bold;margin-left:10rpx}
.check-progress{width:540rpx;height:212rpx;padding:53rpx 75rpx 0 75rpx;position:absolute;top:102rpx;left:50%;margin-left:-345rpx;font-family:PingFangSC-Medium;font-size:30rpx;color:#6A3C12;text-align:center;}
.progress-box{margin-top:25rpx;}
.progress-box text{display:inline-block;width:50rpx;height:12rpx;background:#FFD927;margin-right:5rpx;}
.progress-box text:first-of-type{border-radius:6rpx 0 0 6rpx;}
.progress-box text:last-of-type{border-radius:0 6rpx  6rpx 0;margin-right:0rpx;}
.progress-box text.had-number{background:#FE5201;}
.check-progress .my-team{font-family:PingFangSC-Medium;font-size:30rpx;color:#6A3C12;text-align:center;}
.check-progress .my-team .num{font-family:Helvetica;font-size:40rpx;color:#6A3C12;}
.page .juyuan{position:absolute;top:41.5%;width:400rpx;height:80rpx;left:50%;margin-left:-200rpx;}
.juyuan .share-btn{font-family:PingFangSC-Semibold;font-size:30rpx;color:#FFFFFF;text-align:center;line-height:80rpx;background-color:rgba(0,0,0,0); /* IE6和部分IE7内核的浏览器(如QQ浏览器)会读懂，但解析为透明 */}