page{box-sizing:border-box;height: 100%;background-color: #fbfbfb}

.tablist{height: 100rpx;background-color: #fff}
.tablist .tabItem{position: relative;font-size: 28rpx;color: #333;padding:0 30rpx;height: 100rpx;line-height: 100rpx;text-align: center;}
.tablist .tabItem .line{width:72rpx;height: 4rpx;background-color: #FFDC00;border-radius: 2rpx;position:absolute;bottom: 16rpx;left: 50%;margin-left: -36rpx;display: none;}
.tablist .tabItem.on{color:#333;font-size: 30rpx;font-weight: bold;}
.tablist .tabItem.on .line{display: block;}

.item{background-color: #fff;position:relative;border-radius: 8rpx;overflow: hidden;margin-bottom: 20rpx;padding:24rpx 30rpx;}
.userImg{width: 60rpx;height: 60rpx;border-radius: 50%;margin-right: 20rpx;}
.name{font-size: 28rpx;margin-top: 10rpx;color: #333;width: 230rpx;white-space: nowrap; overflow: hidden; text-overflow: ellipsis}
.remark{font-size:24rpx;margin-top:20rpx;color: #666;}
.income{margin-top: 30rpx;}
.income text{font-size: 24rpx;color: #333;}
.income text.r{margin-left: 40rpx}
.time{position: absolute;top: 36rpx;right: 20rpx;font-size: 20rpx;color: #999}
.nodataDiv{padding-top: 260rpx;}
.explain-text-box{
  padding: 0 30rpx;
  height: 120rpx;
  background-color: #FDF6D8;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #533900;
  letter-spacing: 0;
  text-align: left;
  position: relative;
}
.explain-text{
  width: 100%;
}
.explain-text image{
  width: 30rpx;
  height: 30rpx;
  margin-top: -15rpx
}
.explain-text .text-infor{
  width: 648rpx;
  line-height: 40rpx;
  margin-left: 20rpx;
  margin-top: 20rpx
}
