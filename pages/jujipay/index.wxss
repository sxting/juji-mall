/**
    * bounce
    */

.triple-bounce {
  width: 60rpx;
  height: 16rpx;
  display: flex;
  align-items: center;
  margin-left: 10rpx;
  margin-top: 5rpx;
}

.triple-bounce-bounce1, .triple-bounce-bounce2, .triple-bounce-bounce3 {
  width: 16rpx;
  height: 16rpx;
  background: #6a3c12;
  border-radius: 100%;
  display: inline-block;
  animation: triple-bounce 1.4s infinite ease-in-out both;
}

.triple-bounce-bounce1 {
  animation-delay: -0.32s;
}

.triple-bounce-bounce2 {
  animation-delay: -0.16s;
}

.white-bounces {
  width: 90rpx;
  height: 20rpx;
  display: flex;
  align-items: center;
}

.white-bounce1, .white-bounce2, .white-bounce3 {
  width: 20rpx;
  height: 20rpx;
  margin: 0 5rpx;
  background: #fff;
  border-radius: 100%;
  display: inline-block;
  animation: triple-bounce 1.4s infinite ease-in-out both;
}

.white-bounce1 {
  animation-delay: -0.32s;
}

.white-bounce2 {
  animation-delay: -0.16s;
}

@keyframes triple-bounce {
  0%, 80%, 100% {
    transform: scale(0);
  }

  40% {
    transform: scale(1.0);
  }
}

