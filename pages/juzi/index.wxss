.qd-title{font-size:44rpx;line-height:44rpx;color:#333;}
.qd-text{font-size:24rpx;line-height:24rpx;color:#333;margin-top:20rpx;}
.vline{width:2rpx;height:60rpx;background:#eee;margin:0 20rpx;}
.qtitle{font-size:30rpx;color:#333;font-weight:bold;}
.point-text-wrap{width:600rpx;margin-top:30rpx;}
.point-text{width:66rpx;font-size:20rpx;color:#666;text-align:center;}
.point-wrap{width:600rpx;margin-top:20rpx;box-sizing:border-box;padding:0 14rpx;}
.point{width:30rpx;height:30rpx;}
.pline{width:40rpx;height:2rpx;background:#dedede;}
.juzigzWrap{position:fixed;top:0;left:0;right:0;bottom:0;width:100%;height:100%;background:rgba(0,0,0,0.5);}
.juzigz{position:absolute;top:50%;left:50%;width:590rpx;height:534rpx;margin-left:-295rpx;margin-top:-267rpx;background:#fff;border-radius:10rpx;box-sizing:border-box;}
.line{position:absolute;bottom:94rpx;width:100%;height:1rpx;background:#dedede;}
.pointDiv{height:auto;overflow:hidden;margin:90rpx 30rpx 0;padding:30rpx 40rpx;box-sizing:border-box;background:#FFFFFF;box-shadow:0 0 5rpx 0 rgba(228,228,228,0.50);border-radius:10rpx;}
.pointStatus{text-align:center;font-size:30rpx;color:#333333;margin:50rpx auto 20rpx;font-weight:bold;}

.taskDiv{margin:20rpx 30rpx 0;padding:30rpx 10rpx 10rpx;background:#FFFFFF;box-shadow:0 0 5rpx 0 rgba(228,228,228,0.50);border-radius:10rpx;}
.taskItem{background-color:#fff;height: 150rpx;}
.normalBtn{width:560rpx;height:88rpx;border-radius:44rpx;background:#FFDC00;line-height:88rpx;text-align:center;margin-top:20rpx;}
.signedBtn{width:560rpx;height:88rpx;border-radius:44rpx;background:#f5f5f5;color:#999999;line-height:88rpx;text-align:center;margin-top:20rpx;}
.subImg{width:80rpx;height:80rpx;}
.subCon{margin-right:20rpx;}
.subTit{font-size:28rpx;color:#333333;}
.subTxt{margin-top:6rpx;font-size:24rpx;color:#999999;}
.juziRule{width:160rpx;height:60rpx;text-align:center;line-height:60rpx;font-size:28rpx;color:#ffffff;background-image:linear-gradient(-90deg,#494849 0%,#2D2D2D 100%);border-radius:31rpx;}

.newproductDiv {
  padding: 30rpx;
}
.pubuItem {
  width:48.5%;
  display:flex;
  flex-direction:column;
  box-shadow:0 4rpx 10rpx 0 rgba(228, 228, 228, 0.50);
  border-radius:8rpx;
  padding-bottom:16rpx;
  position:relative;
  overflow:hidden;
  float:left;
  margin-bottom:20rpx;
  background-color:#fff;
  max-height:312rpx;
  height: 312rpx;
}
.newproductDiv .pubuItem:nth-child(even) {
  margin-left:3%;
}
.plimg {
  width:100%;
  height:188rpx;
  position:relative;
  display:block;
  background-color:#e7e7e7;
}

.dis-wrap {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
  padding: 0 30rpx;
}

.from {
  font-size: 22rpx;
  color: #999;
}

.dis {
  font-size: 22rpx;
  color: #999;
}

.pubuname {
  font-size:24rpx;
  color:#333;
  display:-webkit-box;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  padding-left:20rpx;
  padding-right:20rpx;
  height:70rpx;
  line-height:32rpx;
}

.price-wrap {
  display: flex;
  align-items: center;
  margin-top: 14rpx;
  padding: 0 30rpx;
}

.p-wrap {
  display: flex;
  align-items: center;
}

.now-price {
  font-size: 30rpx;
  color: #e83221;
  font-weight: bold;
}

.old-price {
  font-size: 22rpx;
  color: #999;
  text-decoration: line-through;
}

.count {
  font-size: 24rpx;
  color: #999;
  margin-left: 10rpx;
}

.buyBtn{
  width:106rpx;
  height: 50rpx;
  border: 2rpx solid #E83221;
  border-radius: 6rpx;
  font-size: 26rpx;
  color: #E83221;
  display: flex;
  align-items: center;
  justify-content: center;
  margin:0;
  padding: 0;
  box-sizing: border-box;
  background: #fff;
}

.locationWrap {
  background-color: #fff;
  padding: 0 30rpx 30rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.location {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}

.xIcon{
  transition: all .3 ease;
}
.rotate{
  transform: rotate(180deg);
}

.ysqMark{
  width:100%;height:100%;position:absolute;top:0;left:0;background: rgba(0,0,0,.5);
}
.ysqMark image{
  width: 140rpx;
  height: 62rpx;
  position:absolute;
  top:50%;
  left:50%;
  margin-left: -70rpx;
  margin-top: -31rpx;
}

